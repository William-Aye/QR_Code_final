<Page x:Class="QR_Code_final.Affichage.QRCodeAffichage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:QR_Code_final.Affichage"
      mc:Ignorable="d" 
      d:DesignHeight="{x:Static SystemParameters.PrimaryScreenHeight}"
      d:DesignWidth="{x:Static SystemParameters.PrimaryScreenHeight}"
      Title="QRCodeAffichage">

    <Page.Resources>
        <ControlTemplate x:Key="NoMouseOverButtonTemplate" TargetType="Button"/>
        <Style TargetType="Button">
            <Setter Property="Margin" Value="0"/>
        </Style>
    </Page.Resources>

    <Grid Name="GridQR">
        
    </Grid>
</Page>
